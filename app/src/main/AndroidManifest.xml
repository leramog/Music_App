<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.android.music_app">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".MainPage"
            android:label="@string/main_page" />
        <activity
            android:name=".MyAlbums"
            android:label="@string/album" />
        <activity
            android:name=".WhatsNew"
            android:label="@string/Whatnew" />
        <activity
            android:name=".Create"
            android:label="@string/create" />

        <activity android:name=".Concerts"
            android:label="@string/concerts"/>

    </application>

</manifest>